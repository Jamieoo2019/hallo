local Section = Tab:CreateSection("Moin")
local Tab = Window:CreateTab("Credits")
local Section = Tab:CreateSection("Script Made by Jamie")
local Section = Tab:CreateSection("Ui Von Rayfield")
local Section = Tab:CreateSection("NUR FÜR JONAH UND JACK!!")
local Section = Tab:CreateSection("Jack ist ein sigma")
local Section = Tab:CreateSection("Jonah ist ein sigma")
